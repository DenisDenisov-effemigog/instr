{% set showStickyCard = true %}

{% block content %}

    {% include '../../../templates/header/header.twig' with {'categories': categories, 'user': user} %}
    {% include '../../../templates/breadcrumbs.twig' with {
        'breadcrumbs': [
            {
                'title': 'Главная',
                'url': '/'
            },
            {
                'title': 'Силовое оборудование',
                'url': '/PowerEquipment/'
            },
            {
                'title': 'Электроинструменты',
                'url': '/PowelTools/'
            },
            {
                'title': 'Дрели',
                'url': '/Drills/'
            },
        ]
    } %}
    {% include '../../../templates/product-card/product-card.twig' with {'product': product} %}
    {% include '../../../templates/product-card/same-products.twig' with {'productsSames': productsSames, 'user': user} %}
    {% include '../../../templates/product-card/product-tabs.twig' with {
        'product': product,
        'category_discount': category_discount,
        'user': user,
        'productId': product.id
    } %}
    {% include '../../../templates/product-card/recommend.twig' with {'productsRecommended': productsRecommended} %}
    {% include '../../../templates/product-card/banner.twig' with {
        'banner': '/demo_images/banners/product-card.png', 
        'bannerMobile': '/demo_images/banners/mob-banner.png'
    } %}
    {% include '../../../templates/footer/footer.twig'  with {
        'countries': countries,
        'activeCountry': 'Россия',
        'user': user
    } %}
    
{% endblock %}